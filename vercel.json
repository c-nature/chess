{
  "rewrites": [
    {
      "source": "/lib/:path*",
      "destination": "/lib/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/images/:path*"
    }
  ],
  "headers": [
    {
      "source": "/lib/:path*.wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/lib/:path*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/images/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}